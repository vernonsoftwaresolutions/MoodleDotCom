{
  "name": "moodoe-dot-com",
  "version": "0.0.0",
  "license": "MIT",
  "config": {
    "s3BucketName": "lambda.bucket.moodle",
    "nonProdStackName": "nonprod-hosted-zone",
    "prodStackName": "prod-hosted-zone",
    "domainName": "vssdevelopment.com",
    "devSiteName": "dev.moodle.vssdevelopment.com",
    "stageSiteName": "stage.moodle.vssdevelopment.com"


  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build-dev": "ng build --env=dev",
    "build-stage": "ng build --env=stage",
    "build-prod": "ng build --env=prod",
    "test": "ng test --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy-dev": "aws s3 sync --delete ./dist s3://dev.moodle.vssdevelopment.com",
    "deploy-stage": "aws s3 sync --delete ./dist s3://stage.moodle.vssdevelopment.com",
    "install-angular": "npm install -g @angular/cli",
    "package-hosted-zone": "aws cloudformation package --template-file route53-template.yaml --output-template-file output.yml --s3-bucket $npm_package_config_s3BucketName",
    "deploy-nonprod-hosted-zone": "aws cloudformation deploy --template-file output.yml --stack-name $npm_package_config_nonProdStackName --parameter-overrides DomainName=vssdevelopment.com HostedZoneExists=true Environment=nonprod",
    "deploy-prod-hosted-zone": "aws cloudformation deploy --template-file output.yml --stack-name $npm_package_config_prodStackName --parameter-overrides DomainName=vssdevelopment.com HostedZoneExists=true Environment=prod",
    "deploy-dev-site": "./deploy-environment.sh dev $npm_package_config_nonProdStackName $npm_package_config_s3BucketName $npm_package_config_domainName $npm_package_config_devSiteName true",
    "deploy-stage-site": "./deploy-environment.sh stage $npm_package_config_nonProdStackName $npm_package_config_s3BucketName $npm_package_config_domainName $npm_package_config_stageSiteName true"

  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.0.0-beta.2",
    "@angular/animations": "^4.2.4",
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/forms": "^4.2.4",
    "@angular/http": "^4.2.4",
    "@angular/platform-browser": "^4.2.4",
    "@angular/platform-browser-dynamic": "^4.2.4",
    "@angular/router": "^4.2.4",
    "axios": "^0.16.2",
    "body-parser": "^1.17.2",
    "core-js": "^2.4.1",
    "express": "^4.15.4",
    "lodash": "^4.17.4",
    "ng2-bootstrap-modal": "^1.0.1",
    "rxjs": "^5.4.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.3.2",
    "@angular/compiler-cli": "^4.2.4",
    "@angular/language-service": "^4.2.4",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.1.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3"
  }
}
